{
  "manifest_version": 3,
  "name": "Code Extractor",
  "version": "1.0",
  "description": "Extract code from div with class .view-lines",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": [
    "https://developer.chrome.com/*",
    "https://*.google.com/*",
    "https://cdn.jsdelivr.net/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ]
}
